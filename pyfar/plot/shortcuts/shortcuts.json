    "toggle_cm": {
        "key": ["c"],
        "info": "toggle between linear and logarithmic color data"},
    "toggle_all": {
        "key": ["a"],
        "info": "toggle between plotting all channels and plottinng single channels"},
    "next": {
        "key": ["."],
